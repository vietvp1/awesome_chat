<div class="modal fade" id="notificationsModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Tất cả thông báo.</h4>
            </div>
            <div class="modal-body">
                <div class="mark-notif-as-read">
                        <a href="javascript:void(0)" id="modal-mark-notif-as-read">
                            <strong>Đánh dấu tất cả là đã đọc</strong> 
                        </a>
                </div>
                <hr>
                <div class="scroll-notif">
                    <div class="list-notifications">
                        <% notifications.forEach(function (notification) { %>
                            <li>
                                <%- notification %>
                            </li>
                        <% }) %>
                    </div>
                </div>
                
                <div class="read-more-notif">
                    <div class="read-more-notif-spinner"></div>
                </div>
            </div>
        </div>
    </div>
</div>